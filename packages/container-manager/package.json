    "commander": "^12.1.0",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.7",
    "fastify": "^5.6.0",
    "inquirer": "^12.3.0",
    "pg": "^8.16.3",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/dockerode": "^3.3.28",
    "@types/node": "^24.10.1",
