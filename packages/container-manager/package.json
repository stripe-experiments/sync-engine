{
  "name": "@stripe-sync/container-manager",
  "version": "1.0.0",
  "private": true,
  "description": "CLI GUI tool to manage Stripe Sync Docker containers",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "stripe-sync-manager": "./dist/index.js"
  },
  "scripts": {
